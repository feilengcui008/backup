<div class="about_personal_content">
<div class="content content-about">
  <%- partial('_partial/component/page-header', {location: __('location.about')}) %>

  <% if (typeof(theme.about) !== 'undefined' && theme.about_flag) { %>
  <ul class="about-list">
    <% theme.about.forEach(function(item, i){ %>
      <li class="about-item">
        <% if(item.icon) {%>
          <i class="icon <%= item.icon%> dot dot-<%= i % 5%>"></i>
        <% } else { %>
          <i class="dot dot-<%= i % 5%> "></i>
        <% }%>

        <% if(typeof item.text_value === 'object') {%>
          <% item.text_value.forEach(function(item) {%>
            <div class="text"><%= item%></div>
          <% });%>
        <% } else { %>
          <div class="text">
            <span><%= item.text_key || ''%></span>
            <% if(item.text_key) {%>
              <a rel="noopener noreferrer" target="_blank" class="text-value-url" href="<%= item.text_value_url || ''%>"><%= item.text_value || ''%></a>
            <% } else { %>
              <span><%= item.text_value || ''%></span>
            <% }%>
          </div>
        <% }%>

      </li>
    <% }) %>
  </ul>
  <% } %>
</div>
</div>


<% if (theme.custom_about) { %>
  
