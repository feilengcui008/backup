  <div class="aboutdiv">
    <% theme.about.forEach(function(item, i){ %>
      <p>
        <% if(typeof item.text_value === 'object') {%>
          <% item.text_value.forEach(function(item) {%>
            <div class="descdiv"><%= item%></div>
          <% });%>
        <% } else { %>
          <div class="itemdiv">
            <span class="leftspan"><%= item.text_key || ''%></span>
            <% if(item.text_key) {%>
              <span class="rightspan"><a rel="noopener noreferrer" target="_blank" href="<%= item.text_value_url || ''%>"><%= item.text_value || ''%></a></span>
            <% } else { %>
              <span class="rightspan"><%= item.text_value || ''%></span>
            <% }%>
          </div>
        <% }%>
      </p>
    <% }) %>
  </div>

<% } %>

